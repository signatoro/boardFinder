<FindGroupScreenPref3>
    name: "find_group_pref3"

    MDBoxLayout:
        orientation: "vertical"
        spacing: 10
        padding: 10

        MDBoxLayout:
            size_hint_x: 1
            size_hint_y: .15
            orientation: 'horizontal'
            padding: 0

            AnchorLayout:
                anchor_x: 'left'
                anchor_y: 'bottom'
                MDFillRoundFlatIconButton:
                    text: "Previous Preference"
                    #pos_hint: {"right": 1, "y": 0.5}
                    icon: "arrow-left"
                    on_release: root.class_parent.load_next_pref_page(1)

            AnchorLayout:
                anchor_x: 'right'
                anchor_y: 'bottom'
                MDFillRoundFlatIconButton:
                    text: "Next Preference"
                    pos_hint: {'x': 0.9} # , "y": 0.5
                    halign: 'right'
                    icon: "arrow-right"
                    on_release: root.class_parent.load_next_pref_page(3)